{
  "manifest_version": 3,
  "name": "WppCatchOnline",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["https://web.whatsapp.com/*"],
    "css":["content.css"],
    "js": ["content_script.js"]
    }],
  "action": {
    "default_icon": {
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background":{
    "service_worker": "background.js"
  }
}
