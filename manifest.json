{
  "manifest_version": 3,
  "name": "WppCatchOnline",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["https://web.whatsapp.com/*"],
    "js": ["meu_script.js"]
    }],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
]
}
